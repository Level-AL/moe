package day22_methodReturnValue;

public class Task {

	public static void main(String[] args) {

// Create a method that accepts an array of integers and returns largest value
		
		
		int nums[] = {4,1,2,5,0};
		int maxVal = 0;
		
		
		for (int i = 0; i < 5; i++) {
			if(nums[i] > maxVal) {
				nums[i] = maxVal;
			}
		}
				System.out.println(maxVal);
	}

}
